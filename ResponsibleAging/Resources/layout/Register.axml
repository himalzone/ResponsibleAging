        android:layout_height="wrap_content"
        android:id="@+id/textView3"
        android:textColor="#ff000000"
        android:layout_marginLeft="113.2dp" />
    <Button
        android:text="Re-enter Password here"
        android:layout_width="291.8dp"
        android:layout_height="wrap_content"
