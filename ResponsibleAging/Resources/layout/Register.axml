        android:layout_marginLeft="21.2dp"
        android:layout_marginTop="16.7dp" />
    <TextView
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:layout_width="match_parent"
        android:layout_height="63.9dp"
        android:id="@+id/textView1"
        android:textColor="#ff000000"
        android:layout_marginLeft="23.9dp"
        android:layout_marginTop="15.5dp" />
    <TextView
        android:text="Email"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:layout_width="102.4dp"
        android:layout_height="wrap_content"
        android:id="@+id/textView2"
        android:layout_marginLeft="169.9dp"
        android:textColor="#ff000000" />
    <Button
        android:text="Enter emil here"
        android:layout_width="295.2dp"
        android:layout_height="wrap_content"
        android:id="@+id/button5"
        android:layout_marginLeft="50.5dp" />
    <TextView
        android:text="Password"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:layout_width="102.4dp"
        android:layout_height="wrap_content"
        android:id="@+id/textView2"
        android:layout_marginLeft="169.9dp"
        android:textColor="#ff000000"
        android:layout_marginTop="30.6dp" />
    <Button
        android:text="Enter Password here"
        android:layout_width="295.2dp"
        android:layout_height="wrap_content"
        android:id="@+id/button5"
        android:layout_marginLeft="50.5dp" />
    <TextView
        android:text="Confirm Password"
        android:textAppearance="?android:attr/textAppearanceMedium"
